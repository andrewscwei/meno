{
  "name": "meno",
  "version": "0.36.0",
  "description": "An experimental JavaScript UI library",
  "main": "dist/meno.min.js",
  "unpkg": "dist/meno.min.js",
  "homepage": "http://andrewscwei.github.io/meno",
  "scripts": {
    "test": "echo \"No tests yet\"",
    "clean": "rimraf dist lib",
    "build": "npm run clean && npm run build:lib && npm run build:dev && npm run build:prod",
    "build:lib": "mkdirp lib && ncp src lib",
    "build:dev": "NODE_ENV=development webpack --colors --profile --config ./config/build.conf.js --display-error-details",
    "build:prod": "NODE_ENV=production webpack --colors --profile --config ./config/build.conf.js --display-error-details",
    "build:analyze": "USE_ANALYZER=true npm run build:prod",
    "demo": "npm run clean && npm run build:prod && webpack-dev-server --progress --hot --inline --config demo/config/build.conf.js --content-base public --port 8080",
    "demo:clean": "rimraf .gh-pages",
    "demo:build": "npm run demo:clean && webpack --colors --profile --config demo/config/build.conf.js --display-error-details",
    "demo:dev": "webpack-dev-server --progress --hot --inline --config demo/config/dev.conf.js --content-base public --port 8080",
    "pages": "npm run build && npm run demo:build",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewscwei/meno.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "compression-webpack-plugin": "^3.0.0",
    "debug": "^4.1.1",
    "eslint": "^6.3.0",
    "html-webpack-plugin": "^3.2.0",
    "htmlparser2": "^4.0.0",
    "minuet": "^5.11.4",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.12.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0"
  }
}
