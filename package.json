{
  "name": "meno",
  "version": "0.35.0",
  "description": "An experimental JavaScript UI library",
  "main": "dist/meno.min.js",
  "unpkg": "dist/meno.min.js",
  "homepage": "http://andrewscwei.github.io/meno",
  "scripts": {
    "test": "echo \"No tests yet\"",
    "clean": "rimraf dist lib",
    "build": "npm run clean && npm run build:lib && npm run build:dev && npm run build:prod",
    "build:lib": "mkdirp lib && ncp src lib",
    "build:dev": "NODE_ENV=development webpack --colors --profile --config ./config/build.conf.js --display-error-details",
    "build:prod": "NODE_ENV=production webpack --colors --profile --config ./config/build.conf.js --display-error-details",
    "build:analyze": "USE_ANALYZER=true npm run build:prod",
    "demo": "npm run clean && npm run build:prod && npm start --prefix demo",
    "demo:dev": "npm run dev --prefix demo",
    "pages": "npm run build && npm run build --prefix demo",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Andrew Wei <andrewscwei@gmail.com>",
  "files": [
    "dist",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewscwei/meno.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.0.0",
    "debug": "^4.1.1",
    "eslint": "^6.3.0",
    "htmlparser2": "^4.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.8"
  }
}
